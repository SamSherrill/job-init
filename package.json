{
  "name": "job-init",
  "version": "1.0.0",
  "description": "Powerful and Simple Job Searching App for Jr. Developers",
  "main": "server.js",
  "scripts": {
    "lint": "eslint **/*.js --quiet",
    "test": "npm run lint",
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm run start\"",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamSherrill/job-init.git"
  },
  "author": "Sam Sherrill, Stephanie Smith, Jayson Duff, Jolie Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SamSherrill/job-init/issues"
  },
  "homepage": "https://github.com/SamSherrill/job-init#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1"
  }
}
